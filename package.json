{
  "name": "buildercss",
  "private": true,
  "version": "1.0.0",
  "description": "Semantic HTML templates with pure CSS styling",
  "scripts": {
    "build": "pnpm -r build",
    "build:ontology": "npx @tailwindcss/cli -i ./ontology/ontology.css -o ./dist/ontology.css",
    "build:backgrounds": "mkdir -p dist/backgrounds && cp components/backgrounds/*.js dist/backgrounds/",
    "dev": "pnpm -r --parallel dev"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "tailwindcss": "^4.1.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/buildercss.git"
  },
  "keywords": ["semantic", "html", "css", "templates", "mdx"],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dot-do/buildercss/issues"
  },
  "homepage": "https://github.com/dot-do/buildercss#readme",
  "packageManager": "pnpm@9.14.2"
}
